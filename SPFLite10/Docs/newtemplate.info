[config]
Name=Default PDF Template
category=PDF Documentation
extension=pdf
PageSize=Letter
MarginTop=80
MarginRight=80
MarginLeft=80
MarginBottom=80
ShowTitle=1
ShowHeaders=1
ShowFooters=0
ShowPageNb=1
ShowToc=1

[title]
Font=Tahoma
Color=#17365D
Align=center
Size=40
SpaceBefore=300

[toc]
Indent=20
MaxLevels=9
PageLeaderCharacter=.
Title=Table of contents

[footers]
